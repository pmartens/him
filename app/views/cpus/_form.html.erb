<%= simple_form_for @cpu, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :brand %>
  <%= f.input :processor_number %>
  <%= f.input :instruction_set %>
  <%= f.input :cores %>
  <%= f.input :threads %>
  <%= f.input :frequency %>
  <%= f.input :socket_type %>
  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")), cpus_path, :class => 'btn btn-default' %>
<% end %>

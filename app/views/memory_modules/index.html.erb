<table id="memory_modules">
  <tr>
    <td>
      <h1>Listing Memory modules</h1>
      <table id="datatable">
        <thead>
        <tr>
          <th>Brand</th>
          <th>Model</th>
          <th>Type</th>
          <th>Capacity</th>
          <th>Modules</th>
          <th>Buildform</th>
          <th>Speed</th>
          <th>Information</th>
          <th>Created at</th>
          <th>Update at</th>
          <th colspan="3"></th>
        </tr>
        </thead>
        <tbody>
        <% @memory_modules.each do |memory| %>
            <tr>
              <td><%= memory.brand %></td>
              <td><%= memory.model %></td>
              <td><%= memory.modeltype %></td>
              <td><%= memory.capacity %></td>
              <td><%= memory.modules %></td>
              <td><%= memory.buildform %></td>
              <td><%= memory.speed %></td>
              <td><%= memory.information %></td>
              <td><%= memory.created_at.strftime("%Y-%m-%d %H:%M") %></td>
              <td><%= memory.updated_at.strftime("%Y-%m-%d %H:%M") %></td>
              <td><%= link_to 'Show', memory %></td>
              <td><%= link_to 'Edit', edit_memory_module_path(memory) %></td>
              <td><%= link_to 'Destroy', memory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
        <% end %>
        </tbody>
      </table>
      <br>
      <%= link_to 'New Memory module', new_memory_module_path %>
    </td>
  </tr>
</table>